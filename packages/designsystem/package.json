{
  "name": "@cr-ai/designsystem",
  "version": "0.1.81",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/components/index.js",
  "types": "dist/components/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/designsystem/designsystem.esm.js",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/designsystem/designsystem.esm.js",
      "require": "./dist/designsystem/designsystem.cjs.js"
    },
    "./designsystem.css": {
      "import": "./dist/designsystem/designsystem.css"
    },
    "./components/*": {
      "import": "./dist/components/*.js",
      "types": "./dist/components/*.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CRAI-CRM-AI/CRAI-Designsystem.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "icons": "node scripts/generate-icon-components.js --input src/assets/icons --output src/components/icons --clean-output-folder"
  },
  "dependencies": {
    "@stencil/core": "^4.27.1"
  },
  "devDependencies": {
    "@stencil/core": "^4.27.1",
    "@stencil/react-output-target": "^1.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^24.3.0",
    "svgo": "^4.0.0"
  },
  "license": "MIT"
}
